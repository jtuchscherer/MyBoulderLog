<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:boulderlog='urn:import:org.myboulderlog.client.admin.view'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:DialogBox>
        <g:caption>
            <b>Create a new gym</b>
        </g:caption>
        <g:HTMLPanel>

            <boulderlog:GymEditor ui:field="gymEditor" />

            <g:Button ui:field='cancelButton'>Cancel</g:Button>
            <g:Button ui:field='okButton'>Save</g:Button>
        </g:HTMLPanel>
    </g:DialogBox>
</ui:UiBinder>